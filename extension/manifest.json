{
  "manifest_version": 2,

  "name": "Raud.io",
  "description": "This extension demonstrates a browser action with kittens.",
  "version": "1.0",

  "permissions": [
    "activeTab",
    "tabCapture"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://localhost.com:3000; object-src 'self'"
}
